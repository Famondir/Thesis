@article{caiSurveyMixtureExperts2025a,
  title = {A {{Survey}} on {{Mixture}} of {{Experts}} in {{Large Language Models}}},
  author = {Cai, Weilin and Jiang, Juyong and Wang, Fan and Tang, Jing and Kim, Sunghun and Huang, Jiayi},
  year = {2025},
